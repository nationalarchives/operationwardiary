<% translate = require 't7e' %>


<div class="annotation">
  <select>
    <% for quarter in @choices: %>
      <option value="<%= quarter %>"><%- translate 'span', "quarters.#{quarter}" %></option>
    <% end %>
  </select>
  <option value="other"><%- translate 'span', "common.other" %></option>
</div>
