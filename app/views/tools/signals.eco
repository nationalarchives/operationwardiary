<% translate = require 't7e' %>


<div class="annotation">
  
  <label><%- translate 'span', "common.type" %><br>
    <select name="type">
      <% for type in @types: %>
        <option value="<%= type %>"><%- translate 'span', "signals.#{type}" %></option>
      <% end %>
      <option value="other"><%- translate 'span', "common.other" %></option>
    </select>
  </label>
</div>
