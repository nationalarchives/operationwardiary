<% translate = require 't7e' %>
<% if @recents?: %>
  <%- translate 'h2', "profile.recents" %>
  <ul class="recents">
    <% for recent in @recents: %>
    <li><a href="http://talk.operationwardiary.org/#/subjects/<%= recent.zooniverse_id %>"><img src="<%= recent.location.standard %>"></a></li>
    <% end %>
  </ul>
<% end %>